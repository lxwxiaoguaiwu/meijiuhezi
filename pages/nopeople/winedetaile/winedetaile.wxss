.bwizard-steps {
  margin: 0;
  padding: 0;
  display: flex;
  background: #fff;
  justify-content: center
}
.bwizard-steps li {
  display: flex;
  position: relative;
  margin-right: 30rpx;
  line-height: 22rpx;
  list-style: none;
  /* padding: 24rpx 34rpx 20rpx 60rpx; */
  width: 215rpx;
  height: 50rpx;
  background: #999999;
  zoom: 1;
  display: inline;
  padding-left: 40rpx
}
.bwizard-steps li a{
  font-size: 26rpx;
  color: #fff;
  display: flex;
  justify-content: space-around;
  line-height: 50rpx;

}
.bwizard-steps li:first-child {
  -moz-border-radius: 4rpx 0 0 4rpx;
  -webkit-border-radius: 4prx 0 0 4rpx;
  border-radius: 4rpx 0 0 4rpx;
  padding-left: 12rpx
}

.bwizard-steps li:first-child:before {
  border: 0
}

.bwizard-steps li:last-child {
  -moz-border-radius: 0 4rpx 4rpx 0;
  -webkit-border-radius: 0 4rpx 4rpx 0;
  border-radius: 0 4rpx 4rpx 0;
  margin-right: 0
}

.bwizard-steps li:last-child:after {
  border: 0
}

.bwizard-steps li:before {
  position: absolute;
  left: 0;
  top: 0;
  height: 0;
  width: 0;
  border-bottom: 25rpx inset transparent;
  border-left: 25rpx solid #fff;
  border-top: 25rpx inset transparent;
  content: ""
}

.bwizard-steps li:after {
  position: absolute;
  right: -20rpx;
  top: 0;
  height: 0;
  width: 0;
  border-bottom: 25rpx inset transparent;
  border-left: 25rpx solid #999999;
  border-top: 25rpx inset transparent;
  content: "";
  z-index: 2
}
.bwizard-steps .active {
  color: #fff;
  background: #ea5504
}
.bwizard-steps .active:after {
  border-left-color: #ea5504
}
