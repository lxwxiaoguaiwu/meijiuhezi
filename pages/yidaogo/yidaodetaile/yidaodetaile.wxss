.stepper {  
    width: 180rpx;  
    height: 60rpx;  
    /*给主容器设一个边框*/  
    border: 1rpx solid #ccc;  
    border-radius: 3rpx;  
    display: flex;
    margin-top: 30rpx;
    justify-content: space-between
}  
  
/*加号和减号*/  
.stepper text {  
  width: 50rpx;  
  line-height: 60rpx;  
  text-align: center;  
}  
/*数值*/  
.stepper input {  
  width: 80rpx;  
  height: 50rpx;  
  margin: 0 auto;  
  text-align: center;  
  font-size: 30rpx;  
  /*给中间的input设置左右边框即可*/  
  border-left: 1rpx solid #ccc;  
  border-right: 1rpx solid #ccc;  
}  
  
/*普通样式*/  
.stepper .normal{  
  color: black;  
}  
  
/*禁用样式*/  
.stepper .disabled{  
  color: #ccc;  
} 
